<?xml version="1.0" encoding="UTF-8"?>
<configuration>
   <system.webServer>
      <handlers>
         <add name="iisnode" path="backend/index.js" verb="*" modules="iisnode" />
      </handlers>
      <rewrite>
         <rules>
            <rule name="sendToNodeJS">
               <match url="/*" />
               <action type="Rewrite" url="backend/index.js" />
            </rule>
         </rules>
      </rewrite>
   </system.webServer>
</configuration>
